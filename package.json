{
  "name": "ts-nodash",
  "version": "1.1.6",
  "description": "Provides object manipulation tools.",
  "main": "dist/index.js",
  "scripts": {
    "test": "node scripts/test",
    "tests-with-coverage": "nyc --reporter=lcov --reporter=html alsatian \"./src/**/*.test.ts\" && nyc report",
    "clean": "node scripts/clean",
    "build": "node scripts/build",
    "postinstall": "npm run build",
    "prepublish": "npm run build"
  },
  "author": "Shawn Rapp (BadOPCode)",
  "license": "MIT",
  "dependencies": {
    "shelljs": "^0.8.3",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "alsatian": "3.1.0",
    "nyc": "^14.1.1",
    "tap-bark": "^2.0.0",
    "ts-node": "^8.3.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BadOPCode/NoDash.git"
  },
  "keywords": [
    "object",
    "compare",
    "merge",
    "context",
    "library",
    "typescript"
  ]
}
